{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "app/components/formCheckout/rules/index.js", "app/suscribirse/page.js", "app/components/formCheckout/AddressData.jsx", "app/components/formCheckout/PaymentData.jsx", "app/components/formCheckout/PersonalData.jsx", "app/components/formCheckout/FormCheckout.jsx", "app/components/formCheckout/inputs/CustomTextFields.jsx", "app/api/checkout/route.js", "app/rules/index.js"  ],
  "exclude": [
    "node_modules"
  ]
}
